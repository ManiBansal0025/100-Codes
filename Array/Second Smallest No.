import java.util.*;
class Main{
public static void main(String args[]){
Scanner sc = new Scanner(System.in);
System.out.println("Enter the limit");
int n = sc.nextInt();
int arr[] = new int[n];
for(int i=0;i<n;i++){
arr[i] = sc.nextInt();
}
Arrays.sort(arr);
System.out.println("The smallest no. is " + arr[1]);
}
}
