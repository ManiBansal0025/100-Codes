import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int [][]arr = {
	        {1,2,3,4},
	        {2,5,7,8},
	        {1,3,4,5}
	    };
	    for(int i=0;i<3;i++){
	        int max = arr[i][0];
	        for(int j=0;j<4;j++){
	            max=Math.max(max,arr[i][j]);
	        }
	      System.out.println("Max "+max);  
	    }
	}
}
