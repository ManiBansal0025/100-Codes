import java.util.*;
class Main{
public static void main(String args[]){
Scanner sc = new Scanner(System.in);
System.out.println("Enter the limit");
int n = sc.nextInt();
int total = 0;
int arr[] = new int[n];
for(int i=0;i<n;i++){
arr[i] = sc.nextInt();
total = total+arr[i];
}
System.out.println("The sum of elements are " + total);
}
}
