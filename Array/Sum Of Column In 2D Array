import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int [][]arr = {
	        {1,2,3,4},
	        {2,5,7,8},
	        {1,3,4,5}
	    };
	    for(int i=0;i<4;i++){
	        int sum = 0;
	        for(int j=0;j<3;j++){
	            sum = arr[j][i] + sum;
	        }
	      System.out.println("Sum "+sum);  
	    }
	}
}
