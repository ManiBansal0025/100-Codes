import java.util.Scanner;
class Fibonacci
{
public static void main(String args[])
{
Scanner sc = new Scanner(System.in);
System.out.println("Enter a limit");
int num = sc.nextInt();
int a=0;
for(int i=0;i<=num;i++) {
	if(i==0 || i==1) {
		a = i+a;
		
		System.out.print(a+" ");
	}
	else {
		a = a+
	}
}
}
}
